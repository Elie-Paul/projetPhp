{% extends 'base.html.twig' %}
{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('css/style.css') }}"> 
    <link rel="stylesheet" href="{{ asset('css/login.css') }}">
{% endblock %}
{% block title %}Abonnement{% endblock %}

{% block body %}
    <h1>Abonnement</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ abonnement.id }}</td>
            </tr>
            <tr>
                <th>Contrat</th>
                <td>{{ abonnement.contrat }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ abonnement.date ? abonnement.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CumulAnc</th>
                <td>{{ abonnement.cumulAnc }}</td>
            </tr>
            <tr>
                <th>CumulNouv</th>
                <td>{{ abonnement.cumulNouv }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('abonnement_index') }}">back to list</a>

    <a href="{{ path('abonnement_edit', {'id': abonnement.id}) }}">edit</a>

    {{ include('abonnement/_delete_form.html.twig') }}
{% endblock %}
